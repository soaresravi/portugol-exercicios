programa
{
  funcao inicio()
  {
    caracter letra

    escreva("Digite uma letra: ")
    leia(letra)

    se (letra == "a")
    escreva ("vogal")
     senao se (letra == "e")
     escreva ("vogal")
      senao se (letra == "i")
      escreva ("vogal")
       senao se letra == "o")
       escreva ("vogal")
        senao se (letra == "u") {
        escreva ("vogal")
    } 
    senao {
      escreva ("consoante")
    }
  }
}